^src/allmydata/_version\.py$
# _appname.py is generated at build time, and never checked in
^src/allmydata/_appname\.py$
# src/allmydata/_auto_deps.py is copied into place at build time, and never checked in
^src/allmydata/_auto_deps\.py$

# bin/tahoe and bin/tahoe-script.py are now generated files
^bin/tahoe$
^bin/tahoe-script\.py$

# this file is maintained by the buildbot
^\.buildbot-sourcedata$

# automatically-build dependencies (using the 'build-deps' target) go here
^support
